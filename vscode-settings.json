{
    "diffEditor.codeLens": true,
    "diffEditor.experimental.showMoves": true,
    "editor.codeLensFontFamily": "'Monaspace Krypton', monospace",
    "editor.fontFamily": "'Monaspace Neon', '0xProto', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'liga'",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.inlineSuggest.fontFamily": "'Monaspace Krypton', monospace",
    "editor.semanticHighlighting.enabled": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveWhenNoErrors": true,
    "git.autofetch": true,
    "git.blame.editorDecoration.enabled": true,
    "git.branchValidationRegex": "ENG-",
    "git.enableCommitSigning": true,
    "git.fetchOnPull": true,
    "git.mergeEditor": true,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.fontFamily": "'0xProto Nerd Font Propo'",
    "terminal.integrated.fontLigatures": true,
    "window.commandCenter": true,
    "window.zoomLevel": 0.4,
    "workbench.colorTheme": "Monokai Pro (Filter Machine)",
    "workbench.fontAliasing": "auto",
    "workbench.iconTheme": "Monokai Pro (Filter Machine) Icons"
    // "workbench.settings.useSplitJSON": true
    // "editor.cursorSmoothCaretAnimation": "on",
    // "editor.fontLigatures": true,
    // "terminal.integrated.fontLigatures": true,
}